﻿<log4net debug="true">
	<appender name="File" type="log4net.Appender.FileAppender">
		<file value="test.log" />
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
		</layout>
	</appender>

	<root>
		<level value="ALL" />
		<appender-ref ref="File" />
	</root>

</log4net>
